{
    "name": "debugger",
    "displayName": "Debugger",
    "description": "Model context protocol server for debugging and profiling in VS Code.",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.startServer",
                "title": "Start Server"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/vscode": "^1.73.0",
        "typescript": "^5.8.2"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.11.4",
        "zod": "^3.24.4"
    }
}